{
  "name": "@tw/website-frontend",
  "description": "TradeWindow website",
  "version": "1.0.0",
  "dependencies": {
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "bootstrap": "^4.5.3",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.5.0",
    "gatsby-plugin-emotion": "^5.0.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.6.1",
    "gatsby-plugin-offline": "^3.4.0",
    "gatsby-plugin-react-helmet": "^3.4.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.8.0",
    "gatsby-plugin-typescript": "^2.8.0",
    "gatsby-source-filesystem": "^2.8.0",
    "gatsby-source-strapi": "^0.0.12",
    "gatsby-transformer-sharp": "^2.6.0",
    "husky": "^4.3.5",
    "lottie-react": "^2.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-hubspot-form": "^1.3.7",
    "react-player": "^2.9.0",
    "react-scroll": "^1.8.1",
    "react-transition-group": "^4.4.1",
    "shorthash": "^0.0.2",
    "slugify": "^1.4.6"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@types/react-scroll": "^1.8.2",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.15.0",
    "eslint-plugin-react": "^7.21.5",
    "prettier": "2.1.2",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "cross-env ENABLE_GATSBY_REFRESH_ENDPOINT=true GATSBY_HOT_LOADER=fast-refresh TW_WEBSITE_API_URL=\"http://web-cms:1331\" gatsby develop",
    "docker": "yarn docker:build && yarn docker:push",
    "docker:build": "docker build -t tradewindowdev/web-cms:ui_$(git rev-parse --short HEAD) .",
    "docker:push": "docker push tradewindowdev/web-cms:ui_$(git rev-parse --short HEAD)",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint . --ext .tsx --ext .ts",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "type-check": "tsc --noEmit"
  }
}
